    "@reduxjs/toolkit": "^1.6.1",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-alert": "^7.0.3",
    "react-alert-template-basic": "^1.0.2",
    "react-alert-template-snackbar-material-ui": "^1.0.6",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
